r" type="spacer" hr="true" />			  <!--Skill 2-->			  <field name="skill2_spacer" type="spacer" label="Skill 2" />			  <field name="skill4_hr" type="spacer" hr="true" />			  <field name="title2" type="text" label="Title" default="WordPress" description="Skill Title or space" />			  <field name="data_dimension2" type="text" label="Dimension" default="160" description="Skill Dimension, example: 160" />			  <field name="data_percent2" type="text" label="Percent" default="90" description="Skill Percent, example:90" />			  <field name="data_fgcolor2" type="text" label="fgcolor" default="#d74680" description="Data fgcolor" />			  <field name="data_bgcolor2" type="text" label="bgcolor" default="#dfe8ed" description="Data bgcolor" />			  <field name="data_width2" type="text" label="width" default="7" description="Data width, example:5" />			  <field name="skill5_hr" type="spacer" hr="true" />			   <!--Skill 3-->			  <field name="sklii3_spacer" type="spacer" label="Skill 3" />			  <field name="skill6_hr" type="spacer" hr="true" />			  <field name="title3" type="text" label="Title" default="HTML/CSS" description="Skill Title or space" />			  <field name="data_dimension3" type="text" label="Dimension" default="160" description="Skill Dimension, example: 160" />			  <field name="data_percent3" type="text" label="Percent" default="85" description="Skill Percent, example:90" />			  <field name="data_fgcolor3" type="text" label="fgcolor" default="#15c7a8" description="Data fgcolor" />			  <field name="data_bgcolor3" type="text" label="bgcolor" default="#dfe8ed" description="Data bgcolor" />			  <field name="data_width3" type="text" label="width" default="7" description="Data width, example:5" />				  <field name="skill7_hr" type="spacer" hr="true" />			   <!--Skill 4-->			  <field name="skill4_spacer" type="spacer" label="Skill 4" />			  <field name="skill8_hr" type="spacer" hr="true" />			  <field name="title4" type="text" label="Title" default="UX/UI" description="Skill Title or space" />			  <field name="data_dimension4" type="text" label="Dimension" default="160" description="Skill Dimension, example: 160" />			  <field name="data_percent4" type="text" label="Percent" default="92" description="Skill Percent, example:90" />			  <field name="data_fgcolor4" type="text" label="fgcolor" default="#eb7d4b" description="Data fgcolor" />			  <field name="data_bgcolor4" type="text" label="bgcolor" default="#dfe8ed" description="Data bgcolor" />			  <field name="data_width4" type="text" label="width" default="7" description="Data width, example:5" />				  <field name="skill4_hr" type="spacer" hr="true" />			</fieldset>										<fieldset name="advanced">				<field name="moduleclass_sfx" type="text" default="" label="MODSFX" description="MODSFX_DESC" />				<field name="cache" type="list" default="1" label="COM_MODULES_FIELD_CACHING_LABEL" description="COM_MODULES_FIELD_CACHING_DESC">					<option value="1">JGLOBAL_USE_GLOBAL</option>					<option value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>				</field>				<field name="cache_time" type="text" default="900" label="COM_MODULES_FIELD_CACHE_TIME_LABEL" description="COM_MODULES_FIELD_CACHE_TIME_DESC" />				<field name="cachemode" type="hidden" default="itemid">					<option value="itemid"></option>				</field>			</fieldset>					</fields>	</config></extension>